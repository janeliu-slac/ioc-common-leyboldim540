#
# lakeshore335 substitutions file
#

# Generate autosave request 
file autosave_lakeshore335.tpl-req
{
	{
		TCT = $(TCT)
	}
}

file autosave_lakeshore335_inputs.tpl-req
{
	pattern
	{ TCT,		CHAN	}
	{ $(TCT),	A,		}
	{ $(TCT),	B,		}
}

file autosave_lakeshore335_outputs.tpl-req
{
	pattern
	{ TCT,		OUTPUT	}
	{ $(TCT),	1		}
}

file autosave_lakeshore335_analogs.tpl-req
{
	pattern
	{ TCT,		OUTPUT	}
	{ $(TCT),	2		}
}

