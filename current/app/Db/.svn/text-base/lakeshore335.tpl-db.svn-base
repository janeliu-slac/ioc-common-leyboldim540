record(stringin, "$(TCT):GET_IDN") {
    field(DESC, "Lkshr335, identification")
    field(DTYP, "stream")
    field(INP, "@lakeshore335.proto LSCI:GET_IDN $(PORT)")
    field(PINI, "YES")
}

record(ai, "$(TCT):GET_BLOCK_TEMP") {
    field(DESC, "Lkshr335, tc block temp")
    field(DTYP, "stream")
    field(INP, "@lakeshore335.proto LSCI:GET_TEMP $(PORT)")
    field(SCAN, "$(CSCAN) second")
    field(EGU, "%")
    field(PREC, "2")
    field(ADEL, "0.1")
    field(PINI, "YES")
}

record(mbbi, "$(TCT):GET_MODE") {
    field(DESC, "Lkshr335, control status")
    field(DTYP, "stream")
    field(INP,  "@lakeshore335.proto LSCI:GET_MODE $(PORT)")
    field(SCAN, "$(CSCAN) second")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(ZRST, "Local")
    field(ONST, "Remote")
    field(TWST, "Rem/Lock")
    field(ZRSV, "MINOR")
    field(ONSV, "NO_ALARM")
    field(TWSV, "NO_ALARM")
    field(PINI, "YES")
}

record(mbbo, "$(TCT):PUT_MODE") {
    field(DESC, "Lkshr335, control loc/rem")
    field(DTYP, "stream")
    field(OUT,  "@lakeshore335.proto LSCI:SET_MODE $(PORT)")
    field(SCAN, "Passive")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(ZRST, "Local")
    field(ONST, "Remote")
    field(TWST, "Rem/Lock")
    field(ZRSV, "MINOR")
    field(ONSV, "NO_ALARM")
    field(TWSV, "NO_ALARM")
}

record(stringout, "$(TCT):SEND") {
    field(DESC, "Lkshr335, send string")
    field(DTYP, "stream")
    field(OUT,  "@lakeshore335.proto LSCI:SEND($(TCT)) $(PORT)")
    field(SCAN, "Passive")
}
  
record(stringin, "$(TCT):RCVE") {
    field(DESC, "Lkshr335, send/receive")
    field(DTYP, "Soft Channel")
    field(SCAN, "Passive")
}

record(bi, "$(TCT):GET_TUNE_STAT") {
    field(DESC, "Lkshr335, tuning status")
    field(DTYP, "stream")
    field(INP, "@lakeshore335.proto LSCI:GET_AUTOTUNE($(TCT)) $(PORT)")
    field(ZNAM, "Inactive")
    field(ONAM, "Active")
    field(ZSV,  "NO_ALARM")
    field(OSV,  "MINOR")
    field(PINI, "YES")
}

record(mbbi, "$(TCT):GET_TUNE_OUT") {
    field(DESC, "Lkshr335, tuning output")
    field(DTYP, "Raw Soft Channel")
    field(ZRVL, "1")
    field(ONVL, "2")
    field(ZRST, "Output 1")
    field(ONST, "Output 2")
    field(ZRSV, "NO_ALARM")
    field(ONSV, "NO_ALARM")
}

record(bi, "$(TCT):GET_TUNE_ERR") {
    field(DESC, "Lkshr335, tuning error state")
    field(DTYP, "Raw Soft Channel")
    field(ZNAM, "No Error")
    field(ONAM, "Error")
    field(ZSV,  "NO_ALARM")
    field(OSV,  "MAJOR")
}

record(longin, "$(TCT):GET_TUNE_STAGE") {
    field(DESC, "Lkshr335, tuning stage")
    field(DTYP, "Soft Channel")
}

